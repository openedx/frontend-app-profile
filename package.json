    "lodash.pick": "4.4.0",
    "lodash.snakecase": "4.1.1",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-redux": "7.2.9",
    "react-router": "6.20.1",
    "react-router-dom": "6.20.1",
